<Window x:Class="SVLCmanage.DialogBox.StudentSearchDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SVLCmanage.DialogBox"
        mc:Ignorable="d"
        Height="168.489" Width="348.478" WindowStartupLocation="CenterScreen" WindowStyle="ToolWindow" Background="{StaticResource MenuItemSelectionFill}" Loaded="Window_Loaded" ResizeMode="NoResize" >

    <Grid >
        <TextBox x:Name="txtSearch" Margin="10,48,12,0" TextWrapping="Wrap" KeyDown="txtSearch_KeyDown" Height="36" VerticalAlignment="Top" />
        <Button x:Name="OK" Content="OK" Margin="108,0,0,10"  Height="30" VerticalAlignment="Bottom"  Click="OK_Click" HorizontalAlignment="Left" Width="122"/>
        <TextBlock x:Name="label1" Margin="10,15,0,0" FontSize="14" Foreground="#FFCCD9DA" TextAlignment="Center" FontFamily="Phetsarath OT" HorizontalAlignment="Left" Width="320" Height="33" VerticalAlignment="Top"/>
    </Grid>
</Window>
